<script>
    var currentOpenDiv = "none";
    var openDiv = function(value, force) {

        immediate_refresh(3);

        document.getElementById("newShopDiv").style.display = "none";
        document.getElementById("newEquipDiv").style.display = "none";
        document.getElementById("newNewsDiv").style.display = "none";
        document.getElementById("newModeDiv").style.display = "none";
        //document.getElementById("newServerDiv").style.display = "none";
        document.getElementById("newScoreDiv").style.display = "none";
        document.getElementById("newLoginDiv").style.display = "none";
        document.getElementById("newAccountDiv").style.display = "none";

        if (value === "none") {
            value = "none";
        } else if (value === currentOpenDiv) {
            let temp = value;
            if (force === true) {
                setTimeout(function() {
                    openDiv(temp);
                }, 100);
            }

            value = "none";
        } else if (value == "shop") {
            document.getElementById("newShopDiv").style.display = "block";
        } else if (value == "equip") {
            document.getElementById("newEquipDiv").style.display = "block";
        } else if (value == "news") {
            document.getElementById("newNewsDiv").style.display = "block";
            document.getElementById("newNewsDiv").scrollTop = 0;
        }//else if (value == "server") {
        //    document.getElementById("newServerDiv").style.display = "block";
        //}
        else if (value == "mode") {
            document.getElementById("newModeDiv").style.display = "block";
        } else if (value == "leaderboard") {
            document.getElementById("newScoreDiv").style.display = "block";
        } else if (value == "partner") {
            document.getElementById("newNewsDiv").style.display = "block";
            window.open("#partner", "_self");
        } else if (value == "login") {
            document.getElementById("newLoginDiv").style.display = "block";
        } else if (value == "account") {
            document.getElementById("newAccountDiv").style.display = "block";
        }

        currentOpenDiv = value;
    }

    var closeSpawnMessage = function() {
        document.getElementById('spawnMessage').style.display = "none";
    }
</script>
