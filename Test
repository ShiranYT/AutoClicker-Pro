        <script>
            var changeAccountSection = function(value) {
                document.getElementById('accountPasswordDiv').style.display = 'none';
                document.getElementById('accountLogoutDiv').style.display = 'none';

                document.getElementById('accountPasswordButton').style.textDecoration = 'none';
                document.getElementById('accountLogoutButton').style.textDecoration = 'none';

                if (value == 'password') {
                    document.getElementById('accountPasswordDiv').style.display = 'block';
                    document.getElementById('accountPasswordButton').style.textDecoration = 'underline';
                } else if (value == 'logout') {
                    document.getElementById('accountLogoutDiv').style.display = 'block';
                    document.getElementById('accountLogoutButton').style.textDecoration = 'underline';
                }
            }
            changeAccountSection("logout");

            var changeScoreSection = function(value) {
                document.getElementById('scoreDailyDiv').style.display = 'none';
                document.getElementById('scoreWeeklyDiv').style.display = 'none';
                document.getElementById('scoreAllTimeDiv').style.display = 'none';

                document.getElementById('scoreDailyButton').style.textDecoration = 'none';
                document.getElementById('scoreWeeklyButton').style.textDecoration = 'none';
                document.getElementById('scoreAllTimeButton').style.textDecoration = 'none';

                if (value == 'daily') {
                    document.getElementById('scoreDailyDiv').style.display = 'block';
                    document.getElementById('scoreDailyButton').style.textDecoration = 'underline';
                } else if (value == 'weekly') {
                    document.getElementById('scoreWeeklyDiv').style.display = 'block';
                    document.getElementById('scoreWeeklyButton').style.textDecoration = 'underline';
                } else if (value == 'allTime') {
                    document.getElementById('scoreAllTimeDiv').style.display = 'block';
                    document.getElementById('scoreAllTimeButton').style.textDecoration = 'underline';
                }
            }
            changeScoreSection("daily");

            var changeEquipSection = function(value) {
                document.getElementById('equipColorDiv').style.display = 'none';
                document.getElementById('equipWearDiv').style.display = 'none';
                document.getElementById('equipBackDiv').style.display = 'none';
                document.getElementById('equipRideDiv').style.display = 'none';

                document.getElementById('equipColorButton').style.textDecoration = 'none';
                document.getElementById('equipWearButton').style.textDecoration = 'none';
                document.getElementById('equipBackButton').style.textDecoration = 'none';
                document.getElementById('equipRideButton').style.textDecoration = 'none';

                if (value == 'color') {
                    document.getElementById('equipColorDiv').style.display = 'block';
                    document.getElementById('equipColorButton').style.textDecoration = 'underline';
                } else if (value == 'wear') {
                    document.getElementById('equipWearDiv').style.display = 'block';
                    document.getElementById('equipWearButton').style.textDecoration = 'underline';
                } else if (value == 'back') {
                    document.getElementById('equipBackDiv').style.display = 'block';
                    document.getElementById('equipBackButton').style.textDecoration = 'underline';
                } else if (value == 'ride') {
                    document.getElementById('equipRideDiv').style.display = 'block';
                    document.getElementById('equipRideButton').style.textDecoration = 'underline';
                }
            }
            changeEquipSection('color');

            var changeShopSection = function(value) {
                document.getElementById('shopColorDiv').style.display = 'none';
                document.getElementById('shopWearDiv').style.display = 'none';
                document.getElementById('shopBackDiv').style.display = 'none';
                document.getElementById('shopRideDiv').style.display = 'none';

                document.getElementById('shopColorButton').style.textDecoration = 'none';
                document.getElementById('shopWearButton').style.textDecoration = 'none';
                document.getElementById('shopBackButton').style.textDecoration = 'none';
                document.getElementById('shopRideButton').style.textDecoration = 'none';

                if (value == 'color') {
                    document.getElementById('shopColorDiv').style.display = 'block';
                    document.getElementById('shopColorButton').style.textDecoration = 'underline';
                } else if (value == 'wear') {
                    document.getElementById('shopWearDiv').style.display = 'block';
                    document.getElementById('shopWearButton').style.textDecoration = 'underline';
                } else if (value == 'back') {
                    document.getElementById('shopBackDiv').style.display = 'block';
                    document.getElementById('shopBackButton').style.textDecoration = 'underline';
                } else if (value == 'ride') {
                    document.getElementById('shopRideDiv').style.display = 'block';
                    document.getElementById('shopRideButton').style.textDecoration = 'underline';
                }
            }
            changeShopSection('color');
        </script>
    </div>
</div>
