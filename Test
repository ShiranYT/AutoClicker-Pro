            <script>
                var currentMap = 2;
                var changeArea = function(area) {
                    currentMap = area;

                    document.getElementById("areaButton-0").style.textDecoration = "none";
                    document.getElementById("areaButton-1").style.textDecoration = "none";
                    document.getElementById("areaButton-2").style.textDecoration = "none";
                    document.getElementById("areaButton-3").style.textDecoration = "none";

                    document.getElementById(`areaButton-${area}`).style.textDecoration = "underline";

                    for (i in Mob.list) {
                        Mob.list[i].changeBiome = 0;
                    }

                    for (i in Decoration.list) {
                        Decoration.list[i].changeBiome = 0;
                    }

                    for (i in Food.list) {
                        Food.list[i].changeBiome = 0;
                    }

                    document.getElementById("blackBG").style.display = "block";
                    setTimeout(function() {
                        document.getElementById("blackBG").style.display = "none";
                    }, 100);
                }

                var changeServerSection = function(server) {
                    document.getElementById("naServerSection").style.display = "none";
                    document.getElementById("euServerSection").style.display = "none";
                    document.getElementById("asServerSection").style.display = "none";

                    document.getElementById(`${server}ServerSection`).style.display = "block";

                    document.getElementById("naServerButton").style.textDecoration = "none";
                    document.getElementById("euServerButton").style.textDecoration = "none";
                    document.getElementById("asServerButton").style.textDecoration = "none";

                    document.getElementById(`${server}ServerButton`).style.textDecoration = "underline";

                    let gameMode = 'PVP';
                    if (/as\./.test(window.location.href) || /na\./.test(window.location.href) || /eu\./.test(window.location.href))
                        gameMode = 'Food';
                    else if (/-2\./.test(window.location.href))
                        gameMode = 'Food';
                    else
                        gameMode = 'PVP';

                    document.getElementById('modeButton').innerHTML = gameMode;
                }
            </script>
