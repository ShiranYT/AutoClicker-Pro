        <script>
            var youtubeVideoData = [['b57XjwM2ufs', 'Luke'], ['uKEHerDJP2w', '[LG] Legend Gaming'], ['netRVww-Gxs', 'Happy G@mer'], ['Jaxu9BVoFro', 'Trần Huyền'], ['Vxo4ITpNh0U', '-RoNaLdo-272'], ['DD91dSM-ME0', 'MoroziPlay'], ['YvGD3iddLB0', 'MaxiM_THE_BesT'], ['7SjkMHSyEbo', 'Aileri'], ['ukLj24kTftI', 'manh cuong vn'], ['U5e9JuAwhGk', 'DEV'], ['uCoHiMZmAcU', 'megalodon'], ['VOYU0NNFrLA', 'DarkSlayer'], ['cOcJWc_mxso', 'KingbladeEXE'], ['zN7HB8U-FXE', 'k3lp'], ['agLf0qISZS8', 'Patience'], ['NWZ2yGmQ-0E', 'Song Nguyên...'], ['729k1UvoLnI', 'GZ-Agn3x'], ['0yp8ESxPO_w', 'Godenot']];
            var currentVideo = 0;

            var nextVideo = function() {
                currentVideo++;

                if (currentVideo >= youtubeVideoData.length)
                    currentVideo = 0;

                changeYoutubeVideo(youtubeVideoData[currentVideo]);
            }

            var changeYoutubeVideo = function(data) {

                document.getElementById("youtubeDiv").style.backgroundImage = `url('https://img.youtube.com/vi/${data[0]}/hqdefault.jpg')`;
                document.getElementById("youtubeDiv").onclick = function() {
                    window.open('https://www.youtube.com/watch?v=' + data[0], '_blank');
                }
                ;
                document.getElementById("youtuberName").innerHTML = data[1];
            }
            changeYoutubeVideo(youtubeVideoData[0]);
        </script>
